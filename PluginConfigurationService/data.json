	"HomeMaticHomeKitGarageDoorService": {
		"special": true,
		"channelType": [],
		"description": "",
		"configuration": [{
				"address_sensor_close": {
					"label": "Close sensor",
					"control": "text",
                    "hint": "Homematic address of the sensor which indicates a closed door",
                    "validation":"datapoint"
				}
			},
			{
				"state_sensor_close": {
					"label": "State close sensor",
					"control": "text",
					"hint": "State which is reported by the sensor when the door is closed"
				}
			},
			{
				"address_sensor_open": {
					"label": "Open sensor",
					"control": "text",
					"hint": "Homematic address of the sensor which indicates a open door",
                    "validation":"datapoint"
				}
			},

			{
				"state_sensor_open": {
					"label": "State open sensor",
					"control": "text",
					"hint": "State which is reported by the sensor when the door is open"
				}
			},


			{
				"address_actor_open": {
					"label": "Actor open",
					"control": "text",
					"hint": "Homematic address of the actor which will open the door",
                    "validation":"datapoint"
				}
			},

			{
				"message_actor_open": {
					"label": "Message open",
					"control": "text",
                    "hint": "What to send to the actor to open the door",
                    "default": "{'on': 1,'off': 0}"
				}
			},

			{
				"address_actor_close": {
					"label": "Actor close",
					"control": "text",
					"hint": "Homematic address of the actor which will close the door",
                    "validation":"datapoint"
				}
			},

			{
				"message_actor_close": {
					"label": "Message close",
					"control": "text",
					"hint": "What to send to the actor to close the door",
                    "default": "{'on': 1,'off': 0}"
				}
			},


			{
				"delay_actor_open": {
					"label": "Delay open actor",
					"control": "integer",
                    "hint": "seconds until the open actor will be reseted",
                    "default": 5
				}
			},
			{
				"delay_actor_close": {
					"label": "Delay close actor",
					"control": "integer",
                    "hint": "seconds until the close actor will be reseted",
                    "default": 5
				}
			},
			{
				"sensor_requery_time": {
					"label": "Sensor Time",
					"control": "integer",
                    "hint": "Time (seconds) until the sensor will be queryed again (aka door move time)",
                    "default":30
				}
			}
		]
	},
